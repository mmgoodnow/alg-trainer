{
	"name": "alg-trainer",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^3.6.4",
		"cubejs": "^1.3.2",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.2.0",
		"@vue/cli-plugin-eslint": "~4.2.0",
		"@vue/cli-service": "~4.2.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.7.2",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-vue": "^6.1.2",
		"husky": ">=4",
		"lint-staged": ">=10",
		"prettier": "^2.0.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended",
			"prettier"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,vue}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"prettier": {
		"tabWidth": 2,
		"useTabs": true,
		"htmlWhitespaceSensitivity": "ignore"
	}
}
